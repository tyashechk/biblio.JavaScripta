let x = +prompt("введите x")
let y = +prompt("введите y");
let c = +prompt("введите c");
let d = +prompt("введите d");
let e;
if (x * y > 3) {
    e = Math.max(x, 3 * Math.log(Math.abs(x * y * c * d)));
} else if (x * y >= 0 && x * y <= 3) {
    e = Math.min(x, y, Math.max(c * x, d * y));
} else if (x * y < 0) {
    e = 2 * c * d - x;
}
alert(e)